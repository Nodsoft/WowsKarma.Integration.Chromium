{
    "name": "WowsKarma.Integration.Chromium",
    "description": "Chrome extension for WOWS Karma integration",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["https://wows-numbers.com/player/*", "https://na.wows-numbers.com/player/*", "https://ru.wows-numbers.com/player/*", "https://asia.wows-numbers.com/player/*"], 
            "js": ["content-script.js"]
        }
    ]
}